{
  "name": "redleaf-rc",
  "version": "0.0.2",
  "description": "",
  "port": 3010,
  "main": "./dist",
  "module": "./dist",
  "scripts": {
    "dev": "dumi dev",
    "doc": "dumi build",
    "lint": "eslint --ext .jsx --ext .js --ext .ts --ext .tsx src/",
    "pretty": "prettier --write src/* src/**/* src/**/**/* docs/* docs/**/* docs/**/**/*",
    "commit": "cz",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -r 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/redleaf-fe/redleaf-rc.git"
  },
  "keywords": [],
  "author": "redleaf-fe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/redleaf-fe/redleaf-rc/issues"
  },
  "homepage": "https://github.com/redleaf-fe/redleaf-rc#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/*": [
      "prettier --write"
    ],
    "src/**/*": [
      "prettier --write"
    ],
    "src/**/**/*": [
      "prettier --write"
    ],
    "docs/*": [
      "prettier --write"
    ],
    "docs/**/*": [
      "prettier --write"
    ],
    "docs/**/**/*": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.14.0",
    "classnames": "^2.2.6",
    "dayjs": "^1.10.4",
    "lodash": "^4.17.20",
    "prop-types": "^15.7.2",
    "resize-observer-polyfill": "^1.5.1"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.11",
    "@types/lodash": "^4.14.167",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "conventional-changelog-cli": "^2.1.1",
    "cz-conventional-changelog": "3.3.0",
    "dumi": "^1.0.10",
    "eslint": "^7.10.0",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-react-hooks": "^4.1.2",
    "husky": "^4.3.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "typescript": "^4.2.4"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "files": [
    "dist"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
